{
  "name": "tate-narou-v",
  "description": "縦書き表示 & いくつかの操作を追加",
  "private": true,
  "version": "0.4.2",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "vue-tsc --noEmit",
    "postinstall": "wxt prepare",
    "eslint": "eslint",
    "submit-init": "wxt submit init",
    "upload-secret": "jiti ./scripts/upload-secret.ts",
    "dryrun-win": "wxt submit --dry-run --chrome-zip .output/%npm_package_name%-%npm_package_version%-chrome.zip",
    "submit-win": "wxt submit --chrome-zip .output/%npm_package_name%-%npm_package_version%-chrome.zip",
    "dryrun-unix": "wxt submit --dry-run --chrome-zip .output/${npm_package_name}-${npm_package_version}-chrome.zip",
    "submit-unix": "wxt submit --chrome-zip .output/${npm_package_name}-${npm_package_version}-chrome.zip"
  },
  "dependencies": {
    "@1natsu/wait-element": "^4.1.2",
    "@iconify-prerendered/vue-pepicons-pop": "^0.28.1722795431",
    "@prhm0998/shared": "github:prhm0998/shared#v1.0.4",
    "@tailwindcss/vite": "^4.1.13",
    "@vueuse/core": "^12.5.0",
    "@wxt-dev/auto-icons": "^1.1.0",
    "@wxt-dev/i18n": "^0.2.4",
    "@wxt-dev/module-vue": "^1.0.1",
    "consola": "^3.4.2",
    "daisyui": "^5.1.10",
    "dayjs": "^1.11.13",
    "sass": "^1.85.0",
    "tailwindcss": "^4.1.13",
    "vue": "^3.5.22",
    "wxt": "^0.20.11"
  },
  "devDependencies": {
    "@eslint/css": "^0.13.0",
    "@eslint/js": "^9.35.0",
    "@octokit/rest": "^22.0.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/chrome": "^0.0.280",
    "@types/libsodium-wrappers": "^0.7.14",
    "chalk": "^5.4.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.35.0",
    "eslint-plugin-vue": "^10.4.0",
    "fs": "0.0.1-security",
    "globals": "^16.4.0",
    "jiti": "^2.5.1",
    "libsodium-wrappers": "^0.7.15",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.43.0",
    "vue-tsc": "^2.1.10"
